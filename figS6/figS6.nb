  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"\[Lambda]", ",", " ", "\[Beta]crit"}], "}"}], ",", 
    RowBox[{
     RowBox[{"\[Lambda]", "=", 
      RowBox[{
       RowBox[{"1", "-", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"\[Omega]0", " ", 
           SuperscriptBox["\[ExponentialE]", 
            RowBox[{
             RowBox[{"-", "\[Mu]"}], " ", 
             RowBox[{"\[Tau]", "/", "2"}]}]], 
           FractionBox["1", "\[Rho]"], 
           RowBox[{"Sinh", "[", 
            RowBox[{"\[Rho]", " ", "\[Tau]"}], "]"}]}], ")"}], "2"], "-", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{
           SuperscriptBox["\[ExponentialE]", 
            RowBox[{
             RowBox[{"-", "\[Mu]"}], " ", 
             RowBox[{"\[Tau]", "/", "2"}]}]], 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"Cosh", "[", 
              RowBox[{"\[Rho]", " ", "\[Tau]"}], "]"}], "-", 
             RowBox[{
              FractionBox["\[Mu]", 
               RowBox[{"2", "\[Rho]"}]], 
              RowBox[{"Sinh", "[", 
               RowBox[{"\[Rho]", " ", "\[Tau]"}], "]"}]}]}], ")"}]}], ")"}], 
         "2"]}], "/.", 
       RowBox[{"\[Rho]", "\[Rule]", 
