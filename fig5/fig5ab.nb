            RowBox[{"Log", "[", "2", "]"}]}]], 
          RowBox[{"Log", "[", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"#", "-", "1"}], ")"}], 
            FractionBox[
             RowBox[{"1", "-", "\[Lambda]"}], "\[Lambda]"]}], "]"}]}], ",", 
         RowBox[{
          FractionBox["1", 
           RowBox[{"2", 
            RowBox[{"Log", "[", "2", "]"}]}]], 
          RowBox[{"Log", "[", 
           RowBox[{
            FractionBox[
             RowBox[{"#", "-", "1"}], "#"], 
            FractionBox["1", "\[Lambda]"]}], "]"}]}]}], "}"}], "&"}], "/@", 
      RowBox[{"Array", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"N", "[", 
          SuperscriptBox["\[ExponentialE]", "#"], "]"}], "&"}], ",", 
        "\[Beta]res", ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"Log", "[", "\[Beta]crit", "]"}], ",", 
