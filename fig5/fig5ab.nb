Cell[BoxData[
 RowBox[{
  RowBox[{"infcurve", "[", 
   RowBox[{
   "\[Tau]_", ",", " ", "\[Mu]_", ",", "\[Omega]0_", ",", "\[Beta]max_", ",", 
    " ", "\[Beta]res_"}], "]"}], " ", ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"\[Lambda]", ",", " ", "\[Beta]crit"}], "}"}], ",", 
    RowBox[{
     RowBox[{"\[Lambda]", "=", 
      RowBox[{
       RowBox[{"1", "-", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"\[Omega]0", " ", 
           SuperscriptBox["\[ExponentialE]", 
            RowBox[{
             RowBox[{"-", "\[Mu]"}], " ", 
             RowBox[{"\[Tau]", "/", "2"}]}]], 
           FractionBox["1", "\[Rho]"], 
           RowBox[{"Sinh", "[", 
            RowBox[{"\[Rho]", " ", "\[Tau]"}], "]"}]}], ")"}], "2"], "-", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{
           SuperscriptBox["\[ExponentialE]", 
            RowBox[{
             RowBox[{"-", "\[Mu]"}], " ", 
             RowBox[{"\[Tau]", "/", "2"}]}]], 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"Cosh", "[", 
              RowBox[{"\[Rho]", " ", "\[Tau]"}], "]"}], "-", 
             RowBox[{
              FractionBox["\[Mu]", 
               RowBox[{"2", "\[Rho]"}]], 
              RowBox[{"Sinh", "[", 
               RowBox[{"\[Rho]", " ", "\[Tau]"}], "]"}]}]}], ")"}]}], ")"}], 
         "2"]}], "/.", 
       RowBox[{"\[Rho]", "\[Rule]", 
        SqrtBox[
         RowBox[{
          RowBox[{
           SuperscriptBox["\[Mu]", "2"], "/", "4"}], "-", 
          SuperscriptBox["\[Omega]0", "2"]}]]}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"\[Beta]crit", "=", 
      FractionBox["1", 
       RowBox[{"1", "-", "\[Lambda]"}]]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"{", 
